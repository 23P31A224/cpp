#include <bits/stdc++.h>
using namespace std;

int main() {
	int t; cin>>t;
	while(t--){
	    int n; cin>>n; 
	    vector<int> arr;
	    if(n%2 ==0){
	    int start = n/2;
	   // cout<<start<<endl;
	    while(start>=1){
	        arr.push_back(start);
	        start--;
	    }
	    int start2 = n/2;
	    while(n>start2){
	        arr.push_back(n);
	        n--;
	    }
	    for(int i =0; i<arr.size(); i++){
	        cout<<arr[i]<<" ";
	    }
	    cout<<endl;
	    }
	    else{
	        int start = n/2+1;
	   // cout<<start<<endl;
	    while(start>=1){
	        arr.push_back(start);
	        start--;
	    }
	    int start2 = n/2+1;
	    while(n>start2){
	        arr.push_back(n);
	        n--;
	    }
	    for(int i =0; i<arr.size(); i++){
	        cout<<arr[i]<<" ";
	    }
	    cout<<endl;
	    }
	}

}
