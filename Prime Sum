#include<bits/stdc++.h>
using namespace std;
using ll = long long;
using vi = vector<int>;

void s()
{
    int n;
    cin >> n;
    ll c1 = 0, c2 = 0, c3 = 0;
    for (int i = 0; i < n; ++i)
    {
        int v;
        cin >> v;
        if (v == 1)
        {
            c1++;
        }
        else if (v == 2)
        {
            c2++;
        }
        else
        {
            c3++;
        }
    }

    ll ans = 0;
    ans += (c1 * (c1 - 1)) / 2;
    ans += c1 * c2;
    ans += c2 * c3;
    cout << ans << endl;
}
int main()
{
    ios_base::sync_with_stdio(false);
    int t;
    cin >> t;
    while (t--)
        s();
}
