

bool prime(int n) {
    
    // Write your code here
    if(n<=1) return false;
    if(n==2) return true;
    if(n%2==0) return false;
    else{
        for(int i=3;i*i<=n;i+=2){
            if(n%i==0){
                return false;
            }
        }
        return true;
    }
    // returns a boolean value
}
