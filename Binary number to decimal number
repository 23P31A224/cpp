class Solution {
  public:
    int binaryToDecimal(string &b) {
        int n = b.size();
        int result = 0;
        int power = 1;  

        for (int i = n - 1; i >= 0; i--) {
            if (b[i] == '1') {
                result += power;
            }
            power *= 2;
        }

        return result;
    }
};
