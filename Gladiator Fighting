#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int n;
        cin >> n;
        
       
        int minSum =  (n - 2) ;
       
        
        int maxSum = (n - 2) * (n - 1) / 2;
        
        cout << minSum << " " << maxSum << endl;
    }
    
    return 0;
}
